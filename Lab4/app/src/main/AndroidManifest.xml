<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zhiqiang.lab4">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/shoplist"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".StaticBroadCast">
            <intent-filter>
                <action android:name="com.example.t617.Lab4.MyStaticBroadCast" />
            </intent-filter>
        </receiver>
        <activity android:name=".ItemDetailActivity"
            android:launchMode="singleInstance">
        </activity>

    </application>

</manifest>